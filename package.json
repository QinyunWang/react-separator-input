{
  "name": "react-separator-input",
  "description": "React input component and HOC with automatically added separator.",
  "version": "0.1.3",
  "author": "Qinyun Wang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QinyunWang/react-separator-input.git",
    "branch": "master",
    "platform": "github"
  },
  "bugs": {
    "url": "https://github.com/QinyunWang/react-separator-input/issues"
  },
  "keywords": [
    "react-component",
    "react",
    "currency",
    "input",
    "number",
    "number-format",
    "separator"
  ],
  "scripts": {
    "prepare": "husky install",
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build-dist": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "test:watch": "umi-test --coverage --watch",
    "prepublishOnly": "npm run build-dist"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint src/ --ext .tsx,.ts --fix"
    ]
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.6",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "antd": "^4.23.2",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.0",
    "eslint": "8.22.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.8",
    "gh-pages": "^3.0.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "*",
    "yorkie": "^2.0.0"
  }
}
